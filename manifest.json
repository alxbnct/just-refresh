{
    "name" : "自动刷新",
    "version" : "1.3",
    "description" : "一键切换自动刷新",
    "options_page" : "options.html",
    "permissions" : ["activeTab", "storage"],
    "browser_action": {},
    "commands" : {
        "toggle-refresh" : {
            "suggested_key" : {
                "default" : "Alt+R",
                "mac" : "Alt+R"
            },
            "description" : "切换自动刷新快捷键"
        }
    },
    "background" : {
        "scripts" : ["jquery.min.js", "background.js"],
        "persistent" : false
    },
    "icons": {
        "128": "images/just_refresh128.png"
    },
    "manifest_version" : 2
}
